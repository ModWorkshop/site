{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "biiiiiigmonster/hasin": "^5.0",
        "fileeye/mimemap": "^2.0",
        "genealabs/laravel-model-caching": "^12.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hammerstone/fast-paginate": "^2.0.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "illuminatech/multipart-middleware": "^1.1",
        "jcupitt/vips": "2.5.0",
        "knuckleswtf/scribe": "^5.2.1",
        "laravel/framework": "^12.0",
        "laravel/octane": "^2.10.0",
        "laravel/pulse": "^1.0",
        "laravel/sanctum": "^4.1.1",
        "laravel/scout": "^10.0.2",
        "laravel/socialite": "^5.12.1",
        "laravel/telescope": "^5.9.1",
        "laravel/tinker": "^2.7.3",
        "lcobucci/jwt": "5.6.0",
        "league/flysystem-aws-s3-v3": "^3.10",
        "mailersend/laravel-driver": "^2.7",
        "marceloeatworld/plunk-laravel": "^0.1.1",
        "nickurt/laravel-stopforumspam": "^2.0",
        "sentry/sentry-laravel": "^4.1",
        "socialiteproviders/discord": "^4.1",
        "socialiteproviders/github": "^4.1",
        "socialiteproviders/gitlab": "^4.1",
        "socialiteproviders/steam": "^4.1",
        "spatie/laravel-honeypot": "^4.5",
        "spatie/laravel-morph-map-generator": "^1.2.1",
        "spatie/laravel-query-builder": "^6.2.0",
        "spatie/laravel-validation-rules": "^3.4",
        "teampanfu/laravel-hcaptcha": "^1.1",
        "tebex/tebex-sdk-php": "^1.1",
        "ultrono/laravel-sitemap": "^9.3",
        "z4kn4fein/php-semver": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.21.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^8.4",
        "phpunit/phpunit": "^12.3",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
